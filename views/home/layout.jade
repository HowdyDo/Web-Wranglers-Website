       Title and other stuffs 
    title= title
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='description', content='')
    meta(name='keywords', content='')
    meta(name='author', content='')
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:400,600', rel='stylesheet', type='text/css')
    //
       Stylesheets 
    link(href='/Content/Template/style/bootstrap.css', rel='stylesheet')
    //
       Pretty Photo 
    link(href='/Content/Template/style/prettyPhoto.css', rel='stylesheet')
    //
       Flex slider 
    link(href='/Content/Template/style/flexslider.css', rel='stylesheet')
    //
       Font awesome icon 
    link(rel='stylesheet', href='/Content/Template/style/font-awesome.css')
    //
       Parallax slider 
    link(rel='stylesheet', href='/Content/Template/style/slider.css')
    //
       Refind slider 
    link(rel='stylesheet', href='~/Content/Template/style/refineslide.css')
